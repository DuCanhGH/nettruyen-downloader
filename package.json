{
  "name": "@ducanh2912/nettruyen-dl",
  "version": "1.3.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "rimraf dist && npm run lint && rollup -c --compact",
    "format": "prettier --write",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint --ext ts,tsx,js,jsx,cjs,mjs",
    "lint-staged": "lint-staged",
    "new-changeset": "changeset",
    "publish-packages": "pnpm typecheck && pnpm lint && pnpm build && changeset publish",
    "prepare": "husky install && npm run puppeteer",
    "prepublishOnly": "npm run build",
    "puppeteer": "cd ./node_modules/puppeteer && npm i",
    "typecheck": "tsc --noEmit"
  },
  "bin": "./dist/index.js",
  "keywords": [
    "nettruyen",
    "net truyen",
    "comics"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DuCanhGH/nettruyen-downloader.git"
  },
  "author": "napthedev, DuCanhGH",
  "license": "MIT",
  "dependencies": {
    "axios": "1.3.2",
    "inquirer": "9.1.4",
    "node-html-parser": "6.1.4",
    "ora": "6.1.2",
    "pdfkit": "0.13.0",
    "puppeteer": "19.6.3",
    "puppeteer-extra": "3.3.4",
    "puppeteer-extra-plugin-stealth": "2.11.1",
    "radash": "10.7.0",
    "sharp": "0.31.3",
    "tslib": "2.5.0"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.0",
    "@eslint/eslintrc": "1.4.1",
    "@rollup/plugin-terser": "0.4.0",
    "@rollup/plugin-typescript": "11.0.0",
    "@types/eslint": "8.21.0",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.13.0",
    "@types/pdfkit": "0.12.8",
    "@types/sharp": "0.31.1",
    "@types/shell-quote": "1.7.1",
    "@typescript-eslint/eslint-plugin": "5.51.0",
    "@typescript-eslint/parser": "5.51.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-simple-import-sort": "10.0.0",
    "husky": "8.0.3",
    "lint-staged": "13.1.1",
    "prettier": "2.8.4",
    "prettier-plugin-jsdoc": "0.4.2",
    "rimraf": "4.1.2",
    "rollup": "3.15.0",
    "shell-quote": "1.8.0",
    "terser": "5.16.3",
    "tsx": "3.12.3",
    "typescript": "4.9.5"
  }
}
