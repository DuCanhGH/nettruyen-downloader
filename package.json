{
  "name": "@ducanh2912/nettruyen-dl",
  "version": "1.2.3",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "rimraf dist && npm run lint && rollup -c --compact",
    "format": "prettier --write .",
    "lint": "eslint src --cache --cache-location ./node_modules/.cache/eslint --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "puppeteer": "cd ./node_modules/puppeteer && npm i",
    "prepare": "husky install && npm run puppeteer"
  },
  "bin": "./dist/index.js",
  "keywords": [
    "nettruyen",
    "net truyen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/DuCanhGH/nettruyen-downloader.git"
  },
  "author": "napthedev, DuCanhGH",
  "license": "MIT",
  "dependencies": {
    "axios": "1.1.3",
    "inquirer": "9.1.4",
    "node-html-parser": "6.1.4",
    "ora": "6.1.2",
    "pdfkit": "0.13.0",
    "puppeteer": "19.3.0",
    "puppeteer-extra": "3.3.4",
    "puppeteer-extra-plugin-stealth": "2.11.1",
    "radash": "9.5.0",
    "sharp": "0.31.2",
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "1.3.3",
    "@rollup/plugin-terser": "0.1.0",
    "@rollup/plugin-typescript": "10.0.1",
    "@semantic-release/changelog": "6.0.2",
    "@semantic-release/commit-analyzer": "9.0.2",
    "@semantic-release/git": "10.0.1",
    "@semantic-release/github": "8.0.7",
    "@semantic-release/npm": "9.0.1",
    "@semantic-release/release-notes-generator": "10.0.3",
    "@types/inquirer": "9.0.3",
    "@types/node": "18.11.9",
    "@types/pdfkit": "0.12.8",
    "@types/sharp": "0.31.0",
    "@typescript-eslint/eslint-plugin": "5.45.0",
    "@typescript-eslint/parser": "5.45.0",
    "eslint": "8.28.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-simple-import-sort": "8.0.0",
    "husky": "8.0.2",
    "prettier": "2.8.0",
    "prettier-plugin-jsdoc": "0.4.2",
    "rimraf": "3.0.2",
    "rollup": "3.5.0",
    "semantic-release": "19.0.5",
    "terser": "5.16.0",
    "tsx": "3.12.1",
    "typescript": "4.9.3"
  }
}
